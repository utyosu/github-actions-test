script == '$(function(){$("#datepicker1").datepicker();$("#datepicker2").datepicker();});'

h1 = "Analysis"

form
  input#datepicker1 type="text" name="start_date" value=@start_date
  = "ï½ž"
  input#datepicker2 type="text" name="end_date" value=@end_date
  = " "
  input type="submit" value="GO"

h2 = "Channels"
table border=1
  tr
    td = "Name"
    td = "Use Time (h)"
  - @channel_use_time_list.each do |channel, use_time|
    tr
      td = channel.name
      td = "%.1f" % (use_time.to_f / (60 * 60))

h2 = "Users"
table border=1
  tr
    td = "Name"
    td = "Login Time (h)"
  - @user_login_time_list.each do |user, login_time|
    tr
      td = user.name
      td = "%.1f" % (login_time.to_f / (60 * 60))
